<% layout('layout') %>
<div>
  <h1>All Users</h1>
  <% users.map((user) => { %>
  <p><%= user %></p>
  <button id="user-<%= user %>-delete" onclick="deleteUser('<%= user %>')">
    Delete
  </button>
  <button id="user-<%= user %>" onclick="navigateToUser('/user/<%= user %>')">
    Go to page
  </button>
  <% }) %>
</div>
